{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-form": "^1.45.0",
    "@ant-design/pro-layout": "^6.27.1",
    "antd": "^4.16.13",
    "jsoneditor": "^9.5.6",
    "mockjs": "^1.1.0",
    "react": "17.x",
    "react-dom": "17.x",
    "rx-immer": "^0.1.2",
    "rx-immer-react": "^0.1.0",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/jsoneditor": "^9.5.0",
    "@types/lodash": "^4.14.176",
    "@types/mockjs": "^1.0.4",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "yorkie": "^2.0.0"
  }
}
