{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-form": "^1.49.3",
    "@ant-design/pro-layout": "^6.31.2",
    "antd": "^4.17.2",
    "immer": "^9.0.7",
    "jsoneditor": "^9.5.7",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "react": "17.x",
    "react-dom": "17.x",
    "rx-immer": "^0.3.1",
    "rx-immer-react": "^0.3.1",
    "rxjs": "^7.4.0",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/jsoneditor": "^9.5.1",
    "@types/lodash": "^4.14.177",
    "@types/mockjs": "^1.0.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.20",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.0",
    "typescript": "^4.5.2",
    "yorkie": "^2.0.0"
  }
}
